datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Subscriber {
  id        String   @id @default(uuid())
  email     String
  createdAt DateTime @default(now())
}

model Admin {
  id        String   @id @default(uuid())
  email     String
  password  String
}
